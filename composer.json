{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.0.8",
        "friendsofsymfony/rest-bundle": "^2.3",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer-bundle": "^2.3",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/console": "^3.3",
        "symfony/framework-bundle": "^3.3",
        "symfony/web-server-bundle": "^3.3",
        "symfony/yaml": "^3.3",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "symfony/flex": "^1.0",
        "symfony/dotenv": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "php bin/console",
            "assets:install --symlink --relative %PUBLIC_DIR%": "php bin/console"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01C0YR4FDM8YZN0TS8MX8BB5AW",
            "allow-contrib": false
        }
    }
}
